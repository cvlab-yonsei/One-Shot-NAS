diff-so-fancy.markemptylines=false
diff-so-fancy.changehunkindicators=true
diff-so-fancy.stripleadingsymbols=false
diff.sqlite3.binary=true
diff.sqlite3.textconv=echo .dump | sqlite3
merge.conflictstyle=diff3
