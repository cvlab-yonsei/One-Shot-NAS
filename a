pager.log=less -FE
log.mailmap=true
alias.history=log --graph --decorate   --pretty=format:'%C(yellow)%h%Creset  %C(auto)%d%Creset %s  %Cgreen(%ar) %C(bold blue)<%an>%Creset'   --abbrev-commit --date=short --date-order -n30
alias.co=checkout
alias.unstage=reset HEAD --
alias.discard=checkout --
alias.update-branch=branch -f --no-track
alias.ub=branch -f --no-track
alias.assume-unchanged=update-index --assume-unchanged
alias.unassume-unchanged=update-index --no-assume-unchanged
alias.list-assume-unchanged=!sh -c 'git ls-files -v | grep ^[a-z] | sed s/^..//'
alias.list-conflicts=diff --name-only --diff-filter=U
alias.amend=commit --amend -v
alias.wdiff=diff --color-words
alias.wshow=show --color-words
alias.sdiff=! git diff $@ | cdiff -s -w0 
